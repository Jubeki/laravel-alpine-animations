{
    "name": "jubeki/laravel-alpine-animations",
    "description": "Provide Blade Component for customizable Alpine Animations",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/Jubeki/laravel-alpine-animations",
    "authors": [
        {
            "name": "Julius Kiekbusch",
            "email": "contact@julius-kiekbusch.de"
        }
    ],
    "keywords": [
        "laravel",
        "blade",
        "components",
        "alpinejs",
        "animations"
    ],
    "autoload": {
        "psr-4": {
            "Jubeki\\AlpineAnimations\\": "src"
        }
    },
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0 || ^10.0"
    },
    "require-dev": {
        "jubeki/laravel-code-style": "^1.0.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jubeki\\AlpineAnimations\\AlpineAnimationsServiceProvider"
            ]
        }
    },
    "scripts": {
        "check-style": "php-cs-fixer fix --dry-run --diff",
        "fix-style": "php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
